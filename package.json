{
  "name": "coi-ng-ecko",
  "version": "0.0.0",
  "scripts": {
    "ng": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng",
    "------ SERVE ------": "",
    "prestart": "node ./scripts/validate-packages.js && node ./scripts/fixed-version-packages.js",
    "start": "npm run ng serve -- --host 0.0.0.0",
    "sass": "sass --watch src/assets/ --no-source-map",
    "start-prod": "npm run ng serve -- -c production --host 0.0.0.0",
    "------ BUILD ------": "",
    "build": "npm run ng  build",
    "build-prod": "npm run ng build -- -c production",
    "------ UTILS ------": "",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-stats": "npm run ng build -- -c production --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "prettier": "prettier --write \"src/**/*.{ts,html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/material": "17.3.10",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@ngneat/until-destroy": "9.2.3",
    "@ngrx/effects": "17.2.0",
    "@ngrx/entity": "17.2.0",
    "@ngrx/store": "17.2.0",
    "@ngrx/store-devtools": "17.2.0",
    "echarts": "5.5.1",
    "material-symbols": "0.27.2",
    "ngx-echarts": "19.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.11",
    "@angular/cli": "17.3.11",
    "@angular/compiler-cli": "17.3.12",
    "@types/jasmine": "5.1.4",
    "chalk": "2.4.2",
    "jasmine-core": "5.1.2",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.3.3",
    "typescript": "5.4.5",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 3,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 180,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}